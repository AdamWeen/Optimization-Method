function [output] = func_618(f,a0,b0,epsilon)
%用0.618法计算函数极小值的函数
%   提供函数形式、区间左右端点以及误差
lambda = [];mu = [];a = [];b=[];a(0) = a0;b(0) = b0;k=0;
lambda(0) = a0+0.382*(b0-a0);mu(0) = a0+0.618*(b0-a0);
while(b(k)-lambda(k)>epsilon&&mu(k)-a(k)>epsilon)
    if(f(lambda(k))>f(mu(k)))
        if(b(k)-lambda(k)<=epsilon)
            output = mu(k);
            break;
        end
    else
        a(k+1)=lambda(k);b(k+1) = b(k);lam
    end




end